  "version": "1.0.0",
  "description": "Click Nova - Next-Gen System Optimization Suite with Cyberpunk Aesthetics",
  "scripts": {
    "start": "node start.js",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "ts-node-dev --respawn --transpile-only server/system-server.ts",
    "build": "tsc && vite build && node scripts/copy-assets.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build:dev": "vite build --mode development",
    "server": "ts-node server/index.ts",
    "server:metrics": "ts-node server/metrics-server.ts",
    "server:optimization": "ts-node server/optimization-server.ts",
    "server:monitoring": "ts-node server/monitoring-server.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run server:metrics\" \"npm run server:optimization\" \"npm run server:monitoring\"",
    "package": "node create-package.js"
  },
